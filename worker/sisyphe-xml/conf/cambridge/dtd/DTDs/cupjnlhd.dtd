<!-- CUPJNLHD DTD v1.3 28/10/1997Changes from v1.2Comments reduced for readabilityMarked sections (OASIS and SSSH) removed<atl> "purpose" values "verso" and "recto" added, values "run" and "aurun"removed<atl> attribute "abbrev" added with values "abatl" and "abau"<aid> optional flag removed from <artinfo> declaration<aff> new attribute "cor" added instead of separate <cor> element<aff> new attribute "pres" added for present address<des> added to Name elements and %namephr, parameter entity<dispart> added for journal specific article type descriptions<crn> is now required<artcon> start and end tags must be present<url> added to %data for web addresses<ead> added to %data and removed from <p>, <aff> and % namephr<spec> added for special journal issue information<header> attribute "dtdver" added for header dtd version--><!-- Alternative article identifier notations --><!NOTATION pii PUBLIC "-//Elsevier Science//NOTATION Publisher ItemIdentifier (PII)//EN" ><!ENTITY % altids "pii?" ><!ELEMENT (pii)        - - (#PCDATA) ><!ATTLIST pii coding NOTATION (pii) #FIXED pii><!-- System Entity references for Entities --><!ENTITY % ISOlat1 SYSTEM "entities/isolat1.ent">%ISOlat1;<!ENTITY % ISOlat2 SYSTEM "entities/isolat2.ent">%ISOlat2;<!ENTITY % ISOnum SYSTEM "entities/isonum.ent">%ISOnum;<!ENTITY % ISOpub SYSTEM "entities/isopub.ent">%ISOpub;<!ENTITY % ISOtech SYSTEM "entities/isotech.ent">%ISOtech;<!ENTITY % ISOgrk1 SYSTEM "entities/isogrk1.ent">%ISOgrk1;<!ENTITY % ISOgrk2 SYSTEM "entities/isogrk2.ent">%ISOgrk2;<!ENTITY % ISOgrk3 SYSTEM "entities/isogrk3.ent">%ISOgrk3;<!ENTITY % ISOamsr SYSTEM "entities/isoamsr.ent">%ISOamsr;<!ENTITY % ISOcyr1 SYSTEM "entities/isocyr1.ent">%ISOcyr1;<!ENTITY % ISOcyr2 SYSTEM "entities/isocyr2.ent">%ISOcyr2;<!ENTITY % ISOamsa SYSTEM "entities/isoamsa.ent">%ISOamsa;<!ENTITY % ISOamsb SYSTEM "entities/isoamsb.ent">%ISOamsb;<!ENTITY % ISOamsc SYSTEM "entities/isoamsc.ent">%ISOamsc;<!ENTITY % ISOamsn SYSTEM "entities/isoamsn.ent">%ISOamsn;<!ENTITY % ISOamso SYSTEM "entities/isoamso.ent">%ISOamso;<!ENTITY % ISObox SYSTEM "entities/isobox.ent">%ISObox;<!ENTITY % ISOdia SYSTEM "entities/isodia.ent">%ISOdia;<!ENTITY % ISOmfrk SYSTEM "entities/isomfrk.ent">%ISOmfrk;<!ENTITY % ISOmscr SYSTEM "entities/isomscr.ent">%ISOmscr;<!ENTITY % ISOmopf SYSTEM "entities/isomopf.ent">%ISOmopf;<!-- Formulae --><!ELEMENT formula           - -  CDATA  -- provisional solution --><!ATTLIST formula  form     (inline|display)  inline                   disc     (math|chem)       math><!-- Text elements --><!ENTITY % p.em.ph "(e1|e2|e3|e4|e5|e6|e7|e8|e9|e10|			e11|e12|e13|e14|e15|e16|e17|			e18|e19|e20|e21|e22|e23|e24|			e25|e26|e27|e28|e29|e30|e31|			e32|e33|e34|e35|e36|e37|e38|			e39|e40|e41|e42|e43|e44|e45|			e46|e47|e48|e49|e50|e51|e52|			e53|e54|e55|e56|e57|e58|e59|			e60|e61|e62|e63|e64|e65|e66|			e67|e68|e69|e70|e71|e72|e73|			e74|e75|e76|e77|e78|e79|e80|			e81|e82|e83|e84|e85|e86|e87|			e88|e89|e90|e91|e92|e93|e94|			e95|e96|e97|e98|e99)"><!ENTITY % data "(#PCDATA | %p.em.ph; | sup | inf | fn | fnr | url | ead)*"><!ELEMENT p                 - -  (%data;)					+(formula)><!ELEMENT fn                - -  (p+)                    -(fn)><!ATTLIST fn       id       ID                #IMPLIED><!ELEMENT fnr               - o  EMPTY><!ATTLIST fnr      rid      IDREF             #IMPLIED><!ELEMENT %p.em.ph;         - -  (#PCDATA)><!ELEMENT (sup | inf)       - -  (#PCDATA | %p.em.ph; | sup | inf | fn | fnr)*><!-- Affiliation elements --><!ELEMENT aff               - -  (%data; | onm | cny)+              +(oid)><!ATTLIST aff        type   (priv | org | cor | pres)org><!ELEMENT (onm | cny)       - -  (%data;)><!ELEMENT ead               - -  (#PCDATA)><!ELEMENT url               - -  (#PCDATA)><!ATTLIST cny       cnycode NAME                                 #IMPLIED><!ELEMENT oid               - o  EMPTY><!ATTLIST oid        id     ID                                   #REQUIRED><!ELEMENT issue             - -  (pinfo, jsinfo?, jinfo, pubinfo)><!-- Publisher Information --><!ELEMENT pinfo             - -  (pnm, loc*)+><!ELEMENT pnm               - -  (#PCDATA)><!ELEMENT loc               - -  (#PCDATA)><!-- Journal series information --><!ELEMENT jsinfo            - -  (jsid, jtl, jsbt?, jalt?, jabt?)><!ELEMENT jsid              - -  (#PCDATA)><!ELEMENT jtl               - -  (#PCDATA)><!ELEMENT jsbt              - -  (#PCDATA)><!ELEMENT jalt              - -  (#PCDATA)><!ELEMENT jabt              - -  (#PCDATA)><!-- Journal information --><!ELEMENT jinfo             - -  (jid, jtl, jsbt?, jalt?, jabt?,                                 issn, cdn?)><!-- Publication information --><!ELEMENT pubinfo           - -  (vid, iid?, cd, spec?)><!ELEMENT cd                - o  (#PCDATA)><!ATTLIST cd		year	NUMBER			#REQUIRED				month	NUMBER			#REQUIRED				day	NUMBER			#IMPLIED><!-- Refinfo information --><!ELEMENT jid               - -  (#PCDATA)><!ELEMENT issn              - -  (#PCDATA)><!ELEMENT cdn               - -  (#PCDATA)><!ELEMENT vid               - -  (#PCDATA)><!ELEMENT iid               - -  (#PCDATA)><!ELEMENT spec              - -  (p+)><!ELEMENT artinfo           - -  (aid, sici?, altid?, artty, dispart?,categ?, figct?,                               tabct?, refct?, ppct?, wrdct?, ppf?, ppl?,                               crn, hst?)><!NOTATION sici PUBLIC "ANSI/NISO Z39.56-1995//NOTATION SICI//EN" ><!ELEMENT sici              - -  (#PCDATA) ><!ATTLIST sici coding NOTATION (sici) #FIXED sici ><!ELEMENT altid             - -  (%altids;) ><!ELEMENT aid               - -  (#PCDATA)><!ELEMENT artty             - o  EMPTY><!ATTLIST artty artty(RA|RV|RL|LP|SC|ER|PI|AB|AD|BR|JP|ED|EA|ME|RE|XX)   #REQUIRED><!ELEMENT dispart           - -  (#PCDATA)><!ELEMENT categ             - -  (#PCDATA)><!ELEMENT figct             - o  EMPTY><!ATTLIST figct      count  NUMBER                               #REQUIRED><!ELEMENT tabct             - o  EMPTY><!ATTLIST tabct      count  NUMBER                               #REQUIRED><!ELEMENT refct             - o  EMPTY><!ATTLIST refct      count  NUMBER                               #REQUIRED><!ELEMENT ppct              - o  EMPTY><!ATTLIST ppct       count  NUMBER                               #REQUIRED><!ELEMENT wrdct             - o  EMPTY><!ATTLIST wrdct      count  NUMBER                               #REQUIRED><!ELEMENT ppf       - -  (#PCDATA)><!ELEMENT ppl               - -  (#PCDATA)><!ELEMENT crn               - -  (#PCDATA)><!ELEMENT hst               - -  (re |  acc | rv | misc)*><!ELEMENT re                - o  EMPTY><!ATTLIST re         year   NUMBER                               #REQUIRED                     month  NUMBER                               #REQUIRED                     day    NUMBER                               #IMPLIED><!ELEMENT acc               - o  EMPTY><!ATTLIST acc        year   NUMBER                               #REQUIRED                     month  NUMBER                               #REQUIRED                     day    NUMBER                               #IMPLIED><!ELEMENT rv                - o  EMPTY><!ATTLIST rv         year   NUMBER                               #REQUIRED                     month  NUMBER                               #REQUIRED                     day    NUMBER                               #IMPLIED><!ELEMENT misc              - -       (%data;)><!ATTLIST misc       year   NUMBER                               #REQUIRED                     month  NUMBER                               #REQUIRED                     day    NUMBER                               #IMPLIED><!-- Abstract --><!ELEMENT abs               - -  (p+)><!ATTLIST abs      language NAME                                 #IMPLIED><!-- Keyword group --><!ELEMENT kwdg              - -  (kwd+)><!ATTLIST kwdg     class    CDATA                                #IMPLIED                   language NAME                                 #IMPLIED><!ELEMENT kwd               - -  (%data;)                 +(formula)><!-- Naming elements --><!ENTITY % namephr "(%data;|degs|fnms|inits|snm|des|roles)+"><!-- Name elements --><!ELEMENT (degs | fnms |          inits | snm |          roles | des)            - -  (#PCDATA)><!-- Title group elements for general headers --><!ELEMENT dedg              - -  (ded?, prs?)><!ELEMENT ded               - -  (%data;)><!-- Author group elements --><!ELEMENT prs               - -  (%data;)><!ELEMENT tig               - -  (atl+)><!ELEMENT atl               - -  (%data;, sbt?)                                                +(formula)><!ATTLIST atl    language   NAME                                 #IMPLIED                 purpose    (normal | verso | recto)             normal                 abbrev     ( abatl | abau)                      #IMPLIED><!ELEMENT sbt               - -  (%data;)><!ELEMENT aug               - -  ((collab | au)+, aff*)><!ELEMENT collab            - -  (%data;)><!ELEMENT au                - -  (%namephr;)             +(orf)><!ELEMENT orf               - o  EMPTY><!ATTLIST orf        rid    IDREF                                #REQUIRED><!-- Conference group elements --><!ELEMENT cng               - -  (cndf | cndl | cnm | cnn | cnp | cns)*><!ELEMENT cndf              - o  EMPTY><!ATTLIST cndf       year   NUMBER                               #REQUIRED                     month  NUMBER                               #REQUIRED                     day    NUMBER                               #IMPLIED><!ELEMENT cndl              - o  EMPTY><!ATTLIST cndl       year   NUMBER                               #REQUIRED                     month  NUMBER                               #REQUIRED                     day    NUMBER                               #IMPLIED><!ELEMENT cnm    	    - -  (%data;)><!ELEMENT cnn    	    - -  (%data;)><!ELEMENT cnp    	    - -  (%data;)><!ELEMENT cns               - -  (%data;)><!ELEMENT cgn    	    - -  (%data;)><!ELEMENT cgs               - -  (%data;)><!ELEMENT genhdr            - -  (artinfo, tig, dedg?, aug+, abs*, kwdg*,                                 cng*, (cgn | cgs)*)><!ATTLIST genhdr   language NAME                           #IMPLIED ><!ELEMENT artcon            - -  (genhdr | jurhdr)><!-- Contribution information for jurisprudence headers --><!-- Special title elements in jurhdr --><!ELEMENT sentence          - o  (court | date | case | magis | atl | annot)*><!ELEMENT court             - o  (#PCDATA)><!ELEMENT date              - o  (#PCDATA)><!ATTLIST date       year   NUMBER                               #IMPLIED                     month  NUMBER                               #IMPLIED                     day    NUMBER                               #IMPLIED><!ELEMENT case              - o  (#PCDATA)><!ELEMENT magis             - o  (judge | prosec | #PCDATA)*><!ELEMENT judge             - o  (%namephr;)><!ELEMENT prosec            - o  (%namephr;)><!ELEMENT annot             - o  (#PCDATA)><!-- Special elements in jurhdr --><!ELEMENT parties           - o  (purs+ & def+)><!ELEMENT purs              - o  (%namephr;|onm)><!ELEMENT def               - o  (%namephr;|onm)><!ELEMENT jurhdr            - o  (artinfo, sentence, abs*,                                 kwdg*, parties?)><!ATTLIST jurhdr   language NAME                                 #REQUIRED><!-- Maximum content of a header --><!ELEMENT header            - -  (issue, artcon)><!ATTLIST header   dtdver        CDATA                           #REQUIRED>