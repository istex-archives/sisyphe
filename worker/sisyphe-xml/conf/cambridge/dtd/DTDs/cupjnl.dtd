ATA  #IMPLIED><!ELEMENT figimg        - o  EMPTY><!ATTLIST figimg        coding    CDATA  #IMPLIED><!ELEMENT tblttl        - -  (%data;)><!ELEMENT tblcapt       - -  (p+)><!ELEMENT tbldisp       - -  (#PCDATA)><!ATTLIST tbldisp       coding    CDATA  #IMPLIED><!ELEMENT tblimg        - o  EMPTY><!ATTLIST tblimg        coding    CDATA  #IMPLIED><!ELEMENT scmttl        - -  (%data;)><!ELEMENT scmcapt       - -  (p+)><!ELEMENT scmdisp       - -  (#PCDATA)><!ATTLIST scmdisp       coding    CDATA  #IMPLIED><!ELEMENT scmimg        - o  EMPTY><!ATTLIST scmimg        coding    CDATA  #IMPLIED><!ELEMENT xref          - -  (%data;)><!ATTLIST xref	     rid    IDREF   #REQUIRED><!-- Reference section elements --><!ELEMENT refs          - -  (stl?, ref*, refs*)><!ELEMENT ref           - -  (link*, (jref | oref))><!ATTLIST ref        id     ID     #REQUIRED><!ELEMENT link          - o  EMPTY><!ATTLIST link       type     (med|isi|doi|cjo)   #REQUIRED                     value    CDATA  #REQUIRED><!ELEMENT jref          - -  (%data; | jtl | issn | vid | iid | refcd |                               ppf | ppl | au | etal | atl | categ | info1 |                               info2 | info3 | info4 | info5)*><!ELEMENT oref          - -  (%data; | rtl | isbn | refcd | pnm | loc |                              onm | ed | au | etal | confttl | ched | chtl | ppf | ppl | ppct |                               info1 | info2 | info3 | info4 | info5)*><!ELEMENT (info1 | info2 | info3 | info4 | info5)    - -  (%data;)><!ELEMENT refcd	        - - (#PCDATA)><!ATTLIST refcd	        year    NUMBER  #REQUIRED			month   NUMBER  #IMPLIED			season  CDATA	#IMPLIED			day     NUMBER  #IMPLIED><!ELEMENT rtl           - -  (%data;)><!ELEMENT isbn          - -  (#PCDATA)><!ELEMENT ed            - -  (%namephr;)><!ELEMENT ched          - -  (%namephr;)><!ELEMENT chtl          - -  (%data;)><!ELEMENT confttl           - -  (#PCDATA)><!ELEMENT etal								- - (%data;)><!-- Contribution information for jurisprudence headers --><!-- Special title elements in jurhdr --><!ELEMENT sentence       - -  (court | date | case | magis | atl | annot)*><!ELEMENT court          - -  (#PCDATA)><!ELEMENT date           - -  (#PCDATA)><!ATTLIST date       year   NUMBER                               #IMPLIED                     month  NUMBER                               #IMPLIED                     day    NUMBER                               #IMPLIED><!ELEMENT case           - -  (#PCDATA)><!ELEMENT magis          - -  (judge | prosec | #PCDATA)*><!ELEMENT judge          - -  (%namephr;)><!ELEMENT prosec         - -  (%namephr;)><!ELEMENT annot          - -  (#PCDATA)><!-- Special elements in jurhdr --><!ELEMENT parties        - -  (purs+ & def+)><!ELEMENT purs           - -  (%namephr;|onm)><!ELEMENT def            - -  (%namephr;|onm)><!ELEMENT jurhdr         - -  (artinfo, sentence, abs*,                                 kwdg*, parties?)><!ATTLIST jurhdr   language NAME                                 #REQUIRED><!-- Header defined as containing ISSUE and ARTCON --><!ELEMENT header         - -  (issue, artcon)><!-- Footer: contains reference section and other sections --><!ELEMENT footer         - -  (sec|refs)+><!-- Overall root element ARTICLE defined as containing a compulsory HEADER, and an optional BODY, and REFS --><!ELEMENT article        - -  (header, body?, footer?)><!ATTLIST article   dtdver        CDATA                           #REQUIRED>